		{ "hook": "filter:topics.getUnreadTids", "method": "getUnreadTids" },
		{ "hook": "filter:account/profile.build", "method": "userBuild" },
		{ "hook": "action:topic.lock", "method": "topicLock" },
		{ "hook": "filter:category.topics.get", "method": "getCategoryTopics" }
